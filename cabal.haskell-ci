ghc-head:               True
no-tests-no-benchmarks: False
unconstrained:          False
hlint:                  True
hlint-options:          --cpp-ansi --cpp-define=HLINT
irc-channels:           irc.freenode.org#haskell-lens

constraint-set unstable
  constraints: rcu +unstable
  tests:       True
  run-tests:   True
  benchmarks:  True
  haddock:     True
